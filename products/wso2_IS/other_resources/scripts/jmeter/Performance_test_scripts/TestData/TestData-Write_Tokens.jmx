            <stringProp name="script">

consumerKey = &quot;consumerkey&quot;+vars.get(&quot;app_count_id&quot;);
consumerSecret = &quot;consumersecret&quot;+vars.get(&quot;app_count_id&quot;); 
username =  vars.get(&quot;usernamePrefix&quot;)+vars.get(&quot;user_count_id&quot;);
password =  vars.get(&quot;password&quot;);
accessToken = vars.get(&quot;accessToken&quot;);
refreshToken = vars.get(&quot;refreshToken&quot;);
tokenInfo = consumerKey+&quot;,&quot;+ consumerSecret+&quot;,&quot;+username+&quot;,&quot;+password+&quot;,&quot;+accessToken+&quot;,&quot;+refreshToken;

//log.info(&quot;AccessToken = &quot;+accessToken);
//log.info(&quot;RefreshToken = &quot;+refreshToken);

responseCode = prev.getResponseCode();

//log.info(&quot;response code= &quot;+responseCode);

try{


    if (responseCode.equals(&quot;200&quot;)) {
	
             vars.put(&quot;tokenInfo&quot;, tokenInfo);
             String filePath = vars.get(&quot;tokenFilePath&quot;);
             FileWriter fstream = new FileWriter(filePath, true);
             BufferedWriter out = new BufferedWriter(fstream);
             out.write(tokenInfo);
             out.newLine();
             out.close();
             log.info(&quot;wrote  tokenInfo to file.&quot;); 
    }
              
               
}catch (Exception e){
      System.err.println(&quot;Error: &quot; + e.getMessage());
              
